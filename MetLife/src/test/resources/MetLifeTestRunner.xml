<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="MetLife Automation Test" parallel="false">

    <parameter name="useCloudEnv" value="true"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="OS X"/>
    <parameter name="os_version" value="Sierra"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="57.0"/>
    <parameter name="url" value="https://www.metlife.com"/>

    <test name = "MetLife">
        <classes>
            <class name="homepage.TestUrlValidity"/>
            <class name="homepage.TestLanguageValidity"/>
        </classes>
    </test>

</suite>
